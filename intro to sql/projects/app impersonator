/* What does the app's SQL look*/
CREATE TABLE SQL (ID INTEGER PRIMARY KEY AUTOINCREMENT,NUMBER_ID INTEGER,
QUERY TEXT);
INSERT INTO SQL (NUMBER_ID,QUERY) VALUES(1,"CREATE");
INSERT INTO SQL (NUMBER_ID,QUERY) VALUES(2,"INSERT");
INSERT INTO SQL (NUMBER_ID,QUERY) VALUES(3,"ORDER BY");
INSERT INTO SQL (NUMBER_ID,QUERY) VALUES(4,"CASE");

SELECT *FROM SQL;
CREATE TABLE QUERY(ID INTEGER PRIMARY KEY AUTOINCREMENT,
QUERY TEXT,NAME TEXT);
INSERT INTO QUERY(QUERY,NAME) VALUES ("ORDER BY","TO ORDER THINGS IN TABLE");
INSERT INTO QUERY(QUERY,NAME) VALUES ("AND","FOR MULTIPLE CONDITIONS");

INSERT INTO QUERY(QUERY,NAME) VALUES ("OR","FOR MULTIPLE CONDITIONS");
INSERT INTO QUERY(QUERY,NAME) VALUES ("CASE","TO DIFFERENTIATE TABLE");
SELECT *FROM QUERY;
SELECT SQL.QUERY AS CONDITION,QUERY.NAME FROM QUERY JOIN SQL
ON QUERY.ID= SQL.NUMBER_ID
WHERE QUERY.QUERY= "CASE" AND "ORDER BY";
UPDATE SQL SET QUERY="UPDATE" WHERE NUMBER_ID = 2;
SELECT *FROM SQL;
DELETE FROM QUERY WHERE ID = 3;
SELECT *FROM QUERY;
ALTER TABLE SQL ADD APP TEXT;
UPDATE SQL SET APP ="SQL" WHERE ID;
SELECT*FROM SQL;
INSERT INTO SQL(NUMBER_ID,QUERY,APP) VALUES (6,"ALTER","SQL");
SELECT *FROM SQL;
